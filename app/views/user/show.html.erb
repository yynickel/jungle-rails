<section class="user-show">

  <header class="page-header">
    <h1>Account Detail</h1>
  </header>

  <article class="user-detail">

    <div class="row">

      <div class="col-sm-4">

      </div>

      <div class="col-sm-8">
        <dl class="dl-horizontal">
          <dt>First Name</dt>
          <dd><%= @user.first_name %></dd>
          <dt>Last Name</dt>
          <dd><%= @user.last_name %></dd>
          <dt>Email</dt>
          <dd><%= @user.email %></dd>
          <dt>Password</dt>
          <dd><%= @user.password %></dd>
        </dl>
      </div>
    </div>

  </article>

</section>
